}

# line: 152, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/cmake.bbclass
cmake_do_configure() {
	if [ "${OECMAKE_BUILDPATH}" ]; then
		bbnote "cmake.bbclass no longer uses OECMAKE_BUILDPATH.  The default behaviour is now out-of-tree builds with B=WORKDIR/build."
	fi

	if [ "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/protocolgen-23.04.0" = "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/build" ]; then
		find /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/build -name CMakeFiles -or -name Makefile -or -name cmake_install.cmake -or -name CMakeCache.txt -delete
	fi

	# Just like autotools cmake can use a site file to cache result that need generated binaries to run
	if [ -e /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/site-file.cmake ] ; then
		oecmake_sitefile="-C /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/site-file.cmake"
	else
		oecmake_sitefile=
	fi

	cmake \
	  -G 'Ninja' -DCMAKE_MAKE_PROGRAM=ninja \
	  $oecmake_sitefile \
	  /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/protocolgen-23.04.0 \
	  -DCMAKE_INSTALL_PREFIX:PATH=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr \
	  -DCMAKE_INSTALL_BINDIR:PATH=bin \
	  -DCMAKE_INSTALL_SBINDIR:PATH=sbin \
	  -DCMAKE_INSTALL_LIBEXECDIR:PATH=libexec \
	  -DCMAKE_INSTALL_SYSCONFDIR:PATH=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/etc \
	  -DCMAKE_INSTALL_SHAREDSTATEDIR:PATH=../com \
	  -DCMAKE_INSTALL_LOCALSTATEDIR:PATH=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/var \
	  -DCMAKE_INSTALL_LIBDIR:PATH=lib \
	  -DCMAKE_INSTALL_INCLUDEDIR:PATH=include \
	  -DCMAKE_INSTALL_DATAROOTDIR:PATH=share \
	  -DPYTHON_EXECUTABLE:PATH=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin/python3-native/python3 \
	  -DPython_EXECUTABLE:PATH=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin/python3-native/python3 \
	  -DPython3_EXECUTABLE:PATH=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin/python3-native/python3 \
	  -DLIB_SUFFIX= \
	  -DCMAKE_INSTALL_SO_NO_EXE=0 \
	  -DCMAKE_TOOLCHAIN_FILE=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/toolchain.cmake \
	  -DCMAKE_NO_SYSTEM_FROM_IMPORTED=1 \
	  -DCMAKE_EXPORT_NO_PACKAGE_REGISTRY=ON \
	  -DFETCHCONTENT_FULLY_DISCONNECTED=ON \
	       -DOE_QMAKE_PATH_PREFIX=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr     -DOE_QMAKE_PATH_HEADERS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include     -DOE_QMAKE_PATH_LIBS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     -DOE_QMAKE_PATH_ARCHDATA=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     -DOE_QMAKE_PATH_DATA=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share     -DOE_QMAKE_PATH_BINS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin     -DOE_QMAKE_PATH_LIBEXECS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib/libexec     -DOE_QMAKE_PATH_PLUGINS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib/plugins     -DOE_QMAKE_PATH_QML=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib/qml     -DOE_QMAKE_PATH_TRANSLATIONS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/translations     -DOE_QMAKE_PATH_DOCS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/doc     -DOE_QMAKE_PATH_SETTINGS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/etc     -DOE_QMAKE_PATH_EXAMPLES=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/examples     -DOE_QMAKE_PATH_TESTS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/tests     -DOE_QMAKE_PATH_HOST_PREFIX=     -DOE_QMAKE_PATH_HOST_BINS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin     -DOE_QMAKE_PATH_HOST_DATA=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     -DOE_QMAKE_PATH_HOST_LIBS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     -DOE_QMAKE_PATH_EXTERNAL_HOST_BINS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin     -DOE_QMAKE_PATH_QT_HEADERS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include     -DOE_QMAKE_PATH_QT_ARCHDATA=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     -DOE_QMAKE_PATH_QT_DATA=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share     -DOE_QMAKE_PATH_QT_BINS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin     -DOE_QMAKE_PATH_QT_TRANSLATIONS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/translations     -DOE_QMAKE_PATH_QT_DOCS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/doc     -DOE_QMAKE_PATH_QT_SETTINGS=/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/etc     -DOE_QMAKE_PATH_QT_EXAMPLES=/home/fish/star64/yocto/