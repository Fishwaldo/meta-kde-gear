# line: 363, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/base.bbclass
base_do_compile() {
	if [ -e Makefile -o -e makefile -o -e GNUmakefile ]; then
		oe_runmake || die "make failed"
	else
		bbnote "nothing to compile"
	fi
}

# line: 343, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/base.bbclass
base_do_configure() {
	if [ -n "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/configure.sstate" -a -e "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/configure.sstate" ]; then
		if [ "`cat /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/configure.sstate`" != "${BB_TASKHASH}" ]; then
			cd /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/build
			if [ "0" != "1" -a \( -e Makefile -o -e makefile -o -e GNUmakefile \) ]; then
				oe_runmake clean
			fi
			# -ignore_readdir_race does not work correctly with -delete;
			# use xargs to avoid spurious build failures
			find /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/build -ignore_readdir_race -name \*.la -type f -print0 | xargs -0 rm -f
		fi
	fi
	if [ -n "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/configure.sstate" ]; then
		mkdir -p `dirname /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/configure.sstate`
		echo ${BB_TASKHASH} > /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/configure.sstate
	fi
}

# line: 376, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/base.bbclass
base_do_install() {
	:
}

# line: 380, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/base.bbclass
