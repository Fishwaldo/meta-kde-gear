	  -Wno-dev
}

# line: 88, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/cmake.bbclass
cmake_do_generate_toolchain_file() {
	if [ "x86_64-linux" = "x86_64-linux" ]; then
		cmake_crosscompiling="set( CMAKE_CROSSCOMPILING FALSE )"
	fi
	cat > /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/toolchain.cmake <<EOF
# CMake system name must be something like "Linux".
# This is important for cross-compiling.
$cmake_crosscompiling
set( CMAKE_SYSTEM_NAME Linux )
set( CMAKE_SYSTEM_PROCESSOR x86_64 )
set( CMAKE_C_COMPILER gcc )
set( CMAKE_CXX_COMPILER g++ )
set( CMAKE_C_COMPILER_LAUNCHER  )
set( CMAKE_CXX_COMPILER_LAUNCHER  )
set( CMAKE_ASM_COMPILER gcc )
find_program( CMAKE_AR ar DOC "Archiver" REQUIRED )

set( CMAKE_C_FLAGS "  -isystem/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include -O2 -pipe" CACHE STRING "CFLAGS" )
set( CMAKE_CXX_FLAGS "  -isystem/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include -O2 -pipe" CACHE STRING "CXXFLAGS" )
set( CMAKE_ASM_FLAGS "  -isystem/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include -O2 -pipe" CACHE STRING "ASM FLAGS" )
set( CMAKE_C_FLAGS_RELEASE "-DNDEBUG" CACHE STRING "Additional CFLAGS for release" )
set( CMAKE_CXX_FLAGS_RELEASE "-DNDEBUG" CACHE STRING "Additional CXXFLAGS for release" )
set( CMAKE_ASM_FLAGS_RELEASE "-DNDEBUG" CACHE STRING "Additional ASM FLAGS for release" )
set( CMAKE_C_LINK_FLAGS "  -isystem/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include -L/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib                         -L/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib                         -Wl,--enable-new-dtags                         -Wl,-rpath-link,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib                         -Wl,-rpath-link,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib                         -Wl,-rpath,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib                         -Wl,-rpath,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib                         -Wl,-O1 -Wl,--allow-shlib-undefined -Wl,--dynamic-linker=/home/fish/star64/yocto/meta-pine64/build/tmp/sysroots-uninative/x86_64-linux/lib/ld-linux-x86-64.so.2" CACHE STRING "LDFLAGS" )
set( CMAKE_CXX_LINK_FLAGS "  -isystem/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include -O2 -pipe -L/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib                         -L/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib                         -Wl,--enable-new-dtags                         -Wl,-rpath-link,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib                         -Wl,-rpath-link,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib                         -Wl,-rpath,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib                         -Wl,-rpath,/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib                         -Wl,-O1 -Wl,--allow-shlib-undefined -Wl,--dynamic-linker=/home/fish/star64/yocto/meta-pine64/build/tmp/sysroots-uninative/x86_64-linux/lib/ld-linux-x86-64.so.2" CACHE STRING "LDFLAGS" )

# only search in the paths provided so cmake doesnt pick
# up libraries and tools from the native build machine
set( CMAKE_FIND_ROOT_PATH  /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native ${CROSS_DIR}   ${EXTERNAL_TOOLCHAIN} /home/fish/star64/yocto/meta-pine64/build/tmp/hosttools)
