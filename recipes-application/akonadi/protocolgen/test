# line: 183, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/base.bbclass
do_deploy_source_date_epoch() {
    mkdir -p /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/deploy-source-date-epoch
    if [ -e /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/source-date-epoch/__source_date_epoch.txt ]; then
        echo "Deploying SDE from /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/source-date-epoch/__source_date_epoch.txt -> /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/deploy-source-date-epoch."
        cp -p /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/source-date-epoch/__source_date_epoch.txt /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/deploy-source-date-epoch/__source_date_epoch.txt
    else
        echo "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/source-date-epoch/__source_date_epoch.txt not found!"
    fi
}

