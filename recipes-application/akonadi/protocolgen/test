}


python __anon_314__home_fish_star64_yocto_meta_pine64_layers_poky____poky_meta_classes_package_deb_bbclass () {
def __anon_314__home_fish_star64_yocto_meta_pine64_layers_poky____poky_meta_classes_package_deb_bbclass(d):
