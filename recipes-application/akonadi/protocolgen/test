# line: 833, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/sstate.bbclass
sstate_create_package() {
	# Exit early if it already exists
	if [ -e /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ ]; then
		touch /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ 2>/dev/null || true
		return
	fi

	mkdir --mode=0775 -p `dirname /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/`
	TFILE=`mktemp /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.XXXXXXXX`

	OPT="-cS"
	ZSTD="zstd -8 -T8"
	# Use pzstd if available
	if [ -x "$(command -v pzstd)" ]; then
		ZSTD="pzstd -8 -p 8"
	fi

	# Need to handle empty directories
	if [ "$(ls -A)" ]; then
		set +e
		tar -I "$ZSTD" $OPT -f $TFILE *
		ret=$?
		if [ $ret -ne 0 ] && [ $ret -ne 1 ]; then
			exit 1
		fi
		set -e
	else
		tar -I "$ZSTD" $OPT --file=$TFILE --files-from=/dev/null
	fi
	chmod 0664 $TFILE
	# Skip if it was already created by some other process
	if [ -h /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ ] && [ ! -e /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ ]; then
		# There is a symbolic link, but it links to nothing.
		# Forcefully replace it with the new file.
		ln -f $TFILE /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ || true
	elif [ ! -e /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ ]; then
		# Move into place using ln to attempt an atomic op.
		# Abort if it already exists
		ln $TFILE /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ || true
	else
		touch /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ 2>/dev/null || true
	fi
	rm $TFILE
}

# line: 902, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/sstate.bbclass
sstate_unpack_package() {
	ZSTD="zstd -T8"
	# Use pzstd if available
	if [ -x "$(command -v pzstd)" ]; then
		ZSTD="pzstd -p 8"
	fi

	tar -I "$ZSTD" -xvpf /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/
	# update .siginfo atime on local/NFS mirror if it is a symbolic link
	[ ! -h /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.siginfo ] || [ ! -e /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.siginfo ] || touch -a /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.siginfo 2>/dev/null || true
	# update each symbolic link instead of any referenced file
	touch --no-dereference /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/ 2>/dev/null || true
	[ ! -e /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.sig ] || touch --no-dereference /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.sig 2>/dev/null || true
	[ ! -e /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.siginfo ] || touch --no-dereference /home/fish/star64/yocto/meta-pine64/build/sstate-cache/universal/.siginfo 2>/dev/null || true
}

# line: 73, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/staging.bbclass
sysroot_stage_all() {
	sysroot_stage_dirs /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/image /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/sysroot-destdir
}

# line: 43, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/staging.bbclass
sysroot_stage_dir() {
	src="$1"
	dest="$2"
	# if the src doesn't exist don't do anything
	if [ ! -d "$src" ]; then
		 return
	fi

	mkdir -p "$dest"
	rdest=$(realpath --relative-to="$src" "$dest")
	(
		cd $src
		find . -print0 | cpio --null -pdlu $rdest
	)
}

# line: 59, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/staging.bbclass
sysroot_stage_dirs() {
	from="$1"
	to="$2"

	for dir in      /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/lib     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share     /sysroot-only       /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/bin     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/sbin     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/bin     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/sbin     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/libexec     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/etc     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/var ; do
		sysroot_stage_dir "$from$dir" "$to$dir"
	done

	# Remove directories we do not care about
	for dir in      /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/man     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/doc     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/info     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/X11/locale     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/applications     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/bash-completion     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/fonts     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/gtk-doc/html     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/installed-tests     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/locale     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/pixmaps     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/terminfo     /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib/protocolgen/ptest ; do
		rm -rf "$to$dir"
	done
}



python __anon_1135__home_fish_star64_yocto_meta_pine64_layers_poky____poky_meta_classes_insane_bbclass () {
