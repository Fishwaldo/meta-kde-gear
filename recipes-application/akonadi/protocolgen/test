# Use qt.conf settings
set( ENV{QT_CONF_PATH} /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/qt.conf )

# We need to set the rpath to the correct directory as cmake does not provide any
# directory as rpath by default
set( CMAKE_INSTALL_RPATH /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib )

# Use RPATHs relative to build directory for reproducibility
set( CMAKE_BUILD_RPATH_USE_ORIGIN ON )

# Use our cmake modules
list(APPEND CMAKE_MODULE_PATH "/home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/share/cmake/Modules/")

# add for non /usr/lib libdir, e.g. /usr/lib64
set( CMAKE_LIBRARY_PATH /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/lib)

# add include dir to implicit includes in case it differs from /usr/include
list(APPEND CMAKE_C_IMPLICIT_INCLUDE_DIRECTORIES /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include)
list(APPEND CMAKE_CXX_IMPLICIT_INCLUDE_DIRECTORIES /home/fish/star64/yocto/meta-pine64/build/tmp/work/x86_64-linux/protocolgen-native/23.04.0-r0/recipe-sysroot-native/usr/include)

EOF
}

