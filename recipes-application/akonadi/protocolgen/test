# line: 320, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/utils.bbclass
check_git_config() {
	if ! git config user.email > /dev/null ; then
		git config --local user.email "oe.patch@oe"
	fi
	if ! git config user.name > /dev/null ; then
		git config --local user.name "OpenEmbedded"
	fi
}

# line: 209, file: /home/fish/star64/yocto/meta-pine64/layers/poky/../poky/meta/classes/cmake.bbclass
cmake_do_compile() {
